    "tokenColors":[
        {
            "name": "css - colons",
            "scope": ["punctuation.separator.key-value", "punctuation.terminator.rule.css"],
            "settings": {
                "foreground": "#e4a561"
            }
        },
        {
            "name": "css - var names",
            "scope": [
            "entity.other.attribute-name.class",
            "entity.name.tag.css"],
            "settings": {
                "foreground": "#E07088",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "css - a:this-text",
            "scope": "entity.other.attribute-name.pseudo-class.css",
            "settings": {
                "foreground": "#becfd4",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "css - values",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#bc85ca"
            }
        },
        {
            "name": "css- commments",
            "scope": "comment.block.css",
            "settings": {
                "foreground": "#581a3a",
                "fontStyle": "italic"
            }
        },

        //JS Start
        
        {
            "name": "js - type declaration - var let const",
            "scope": "storage.type.js",
            "settings": {
                "foreground": "#7595a1",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "js - var names",
            "scope": "",
            "settings": {
                "foreground": "#954ea5"
            }
        },
        {
            "name": "js - if condition",
            "scope": ["keyword.control.conditional.js"],
            "settings": {
                "foreground": "#c1445f",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "js - smol symbols - + : . = <",
            "scope": ["keyword.operator.relational.js", "keyword.operator.arithmetic.js", "punctuation.accessor.js", "keyword.operator.assignment.js"],
            "settings": {
                "foreground": "#e4a561"
            }
        },
        {
            "name": "js - numbers",
            "scope": "constant.numeric.decimal.js",
            "settings": {
                "foreground": "#7b99bd"
            }
        },
        {
            "name": "js - function =>",
            "scope": ["storage.type.function.js", "storage.type.function.arrow.js"],
            "settings": {
                "foreground": "#8E0A91",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "js - return",
            "scope": "keyword.control.flow.js",
            "settings": {
                "foreground": "#148282",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "js - import",
