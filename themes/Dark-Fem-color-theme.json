  "tokenColors": [
    //Html start

    {
      "name": "Html - tag name",
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#bc4c7c",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Html - tag closers <> </>",
      "scope": [
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.begin.html"
      ],
      "settings": {
        "foreground": "#c974ca",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Html - <h>this text</h>",
      "scope": "text.html.derivative",
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Html - double quote",
      "scope": "string.quoted.double.html",
      "settings": {
        "foreground": "#D18960",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Html-- property name - class className",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#f2bfca",
        "fontStyle": "bold"
      }
    },

    //css start

    {
      "name": "css - colons",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.terminator.rule.css"
      ],
      "settings": {
        "foreground": "#e4a561"
      }
    },
    {
      "name": "css - var names",
      "scope": ["entity.other.attribute-name.class", "entity.name.tag.css"],
      "settings": {
        "foreground": "#E07088",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "css - a:this-text",
      "scope": "entity.other.attribute-name.pseudo-class.css",
      "settings": {
        "foreground": "#becfd4",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "css - values",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#bc85ca"
      }
    },
    {
      "name": "css- commments",
      "scope": "comment.block.css",
      "settings": {
        "foreground": "#581a3a",
        "fontStyle": "italic"
      }
    },

    //JS Start

    {
      "name": "js - type declaration - var let const",
      "scope": "storage.type.js",
      "settings": {
        "foreground": "#7595a1",
        "fontStyle": "italic bold"
      }
